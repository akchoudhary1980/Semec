@model Semec.Areas.TenderSearchManage.Model.DepartmentCategoryModel
@{ Layout = "~/Areas/TenderSearchManage/Views/Shared/_Layout.cshtml"; }

<div class="page-header">
    <div class="card">
        <div class="card-block bg-primary">
            <div class="row ">
                <div class="col-lg-4">
                    <h5 class="text-uppercase font-weight-bold">Delete DepartmentCategory</h5>
                </div>
                <div class="col-lg-8 align-items-end">
                    <div class="page-header-breadcrumb">
                        <ul class="breadcrumb-title">
                            <li class="breadcrumb-item">
                                <a href="@Url.Action("Index", "Dashboard")"> <i class="icofont icofont-home" style="color:white"></i> </a>
                            </li>
                            <li class="breadcrumb-item">
                                <a href="@Url.Action("Index", "DepartmentCategory")" style="color:white">Home / Masters / Delete Department Category</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row animate bounceInDown">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <div class="row">
                    <div class="col-md-9 col-9"><h3>@ViewData["PageTitle"]</h3></div>
                    <div class="col-md-3 col-3">
                        <a href="@Url.Action("Index", "DepartmentCategory")" class="btn btn-primary btn-outline-primary float-right">
                            <i class="icofont icofont-user-alt-3"></i> Back to List
                        </a>
                    </div>
                </div>
            </div>

            @using (Html.BeginForm("Delete", "DepartmentCategory", FormMethod.Post, htmlAttributes: new { @class = "swa-confirm" }))
            {
                <div class="card-block">
                    <h4 class="sub-title">DepartmentCategory Delete</h4>
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    @Html.HiddenFor(model => model.DepartmentCategoryID)
                    <input type="hidden" id="confirm" name="confirm" value="No" />
                    <div class="form-group row">
                        @Html.LabelFor(model => model.DepartmentCategoryName, htmlAttributes: new { @class = "control-label col-md-2" })
                        <div class="col-md-10">
                            @Html.EditorFor(model => model.DepartmentCategoryName, new { htmlAttributes = new { @class = "form-control" } })
                            @Html.ValidationMessageFor(model => model.DepartmentCategoryName, "", new { @class = "text-danger" })
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-12">
                            <input type="submit" id="btn_submit" value="Delete" class="btn btn-primary btn-out waves-effect md-trigger float-right" />
                        </div>
                    </div>
                </div>
            }

        </div>
    </div>
</div>





