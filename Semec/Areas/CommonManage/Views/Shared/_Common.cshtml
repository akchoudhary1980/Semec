    <style>
        /*input.ng-pristine {
            border: 1px solid green;
        }*/

        input.ng-touched.ng-invalid {
            border: 1px solid red;
        }

        input.ng-touched.ng-valid {
            background-color: white;
        }

        .requred {
            color: red;
        }
        .requred-lavel {
            color: lightseagreen;
        }
    </style>

    <!-- Js Require -->

    <script src="~/Content/adminty/custome-js/jquery-3.3.1.min.js"></script>
    <script src="~/Content/adminty/custome-js/jquery-ui-1.10.2.min.js"></script>
    <script src="~/Content/adminty/custome-js/ajax-modules.js"></script>
    <script src="~/Content/adminty/custome-js/auto-complete.js"></script>
    <script src="~/Content/adminty/custome-js/sweetalert.min.js"></script>

    <script type="text/javascript">
        function MessageBox(message) {
            return swal({
                title: "Message",
                text: message,
                icon: "success",
                closeOnConfirm: true,
                button: "OK",
            });
        }
    </script>

    <!-- For Create Confirm -->
    @if (Response.Cookies["Create"].Value != null)
    {
        if (Response.Cookies["Create"].Value.ToString() == "Yes")
        {
            <script type="text/javascript">
                window.onload = function () {
                    swal({
                        title: "Record Add!",
                        text: "New record add successfully!",
                        icon: "success",
                        button: "OK",
                    });
                };
            </script>
            Response.Cookies["Create"].Value = "No";
        }
    }
    <!-- For Edit Confirm -->
    @if (Response.Cookies["Edit"].Value != null)
    {
        if (Response.Cookies["Edit"].Value.ToString() == "Yes")
        {
            <script type="text/javascript">
                window.onload = function () {
                    swal({
                        title: "Record Update!",
                        text: "Record update successfully!",
                        icon: "success",
                        button: "OK",
                    });
                };
            </script>
            Response.Cookies["Edit"].Value = "No";
        }
    }
    <!-- For Delete Confirm -->
    <script type="text/javascript">
        $(document).ready(function () {
            $("#btn_submit").click(function (e) {
                e.preventDefault(); //prevent submit
                swal({
                    title: "Are you sure?",
                    text: "Once deleted, you will not be able to recover this record!",
                    icon: "warning",
                    buttons: true,
                    dangerMode: true,

                }).then((willDelete) => {
                    if (willDelete) {
                        $('#confirm').val("Yes");
                        swal("Poof! Your record has been deleted!", { icon: "success", });
                        $('.swa-confirm').submit();
                    } else {
                        $('#confirm').val("No");
                        swal("Your record is safe!");
                    }
                });
            });
        });
    </script>


</head>
<!-- Menu sidebar layout -->
